  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "test",
      "runtimeExecutable": "node",
      "cwd": "${workspaceFolder}",
      "program": "${cwd}/dist/tests/tests.js",
      "env": {
        "EXIFJS_DEVELOPMENT_LOGGING": "1"
      },
      "preLaunchTask": "compound: pretest",
      "console": "integratedTerminal",
      "skipFiles": [
        "<node_internals>/**",
        "node_modules/**"
      ]
    },
  ]
}